{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "now": {
    "alias": "urly"
  },
  "scripts": {
    "start--dev": "webpack -w & nodemon server.js",
    "start": "node server.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "test": "jest --coverage --verbose",
    "test--ci": "jest --coverage --runInBand",
    "test--watch": "jest --watch --verbose",
    "test--snapshots": "jest --updateSnapshot",
    "prettier": "prettier --write 'src/client/**/*.js' 'src/client/**/*.jsx'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/client/tests/setup.js",
    "moduleNameMapper": {
      "\\.(css|styl|less|sass|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/client/tests/__mock__/fileTransformer.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^9.2.11",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fetch-mock": "^7.0.7",
    "file-loader": "^1.1.11",
    "husky": "^1.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-emotion": "^9.2.11",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "react-hot-loader": "^4.3.11",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "node-cache": "^4.2.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "shortid": "^2.2.13",
    "whatwg-fetch": "^3.0.0"
  }
}
